    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "name": "Launch Paradies Bot",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "runtimeArgs": [
                "-r", 
                "ts-node/register", 
                "--experimental-loader",
                "ts-node/esm"
            ],
            "args": [
                "${workspaceFolder}\\index.ts"
            ],
        }
    ]
}